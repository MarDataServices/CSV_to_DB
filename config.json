{

    "*.csv": {
        "primary_key": "id"
    },
    
    "customers.csv": {},
    "products.csv": {},
    "orders.csv": {},
    "order_items.csv": {},
    "payments.csv": {},

    "orders.csv": {
        "foreign_key":{
            "customer_id": "customers(id)"
        }
    },
    "order_items.csv": {
        "foreign_key":{
            "order_id": "orders(id)",
            "product_id": "products(id)"
        }
    },
    "payments.csv": {
        "foreign_key":{
            "order_id": "orders(id)"
        }
    }
}